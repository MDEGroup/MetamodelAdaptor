    <eStructuralFeatures xsi:type="ecore:EAttribute" name="sesso" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EChar"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="indirizzo" lowerBound="1"
