    <eStructuralFeatures xsi:type="ecore:EReference" name="prediletto" eType="#//Padre"
        eOpposite="#//Padre/prediletti"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Padre" eSuperTypes="#//Persona">
    <eStructuralFeatures xsi:type="ecore:EReference" name="famiglia" lowerBound="1"
