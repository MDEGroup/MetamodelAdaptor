        containment="true" eOpposite="#//Padre/famiglia"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="madre" lowerBound="1" eType="#//Madre"
        containment="true" eOpposite="#//Madre/famiglia"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="figli" upperBound="-1"
        eType="#//Figlio" containment="true" eOpposite="#//Figlio/famiglia"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cognome" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
