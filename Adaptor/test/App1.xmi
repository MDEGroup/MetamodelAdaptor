<?xml version="1.0" encoding="ASCII"?>
<webapp1:App
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:webapp1="http://webapp1/1.0"
    xsi:schemaLocation="http://webapp1/1.0 WebApp1.ecore"
    name="Student"
    URL="http://disim.it/students">
  <pages name="Profile">
    <elements
        xsi:type="webapp1:Text"
        name="Name"
        position="1"
        value="myname"/>
    <elements
        xsi:type="webapp1:Text"
        name="Surname"
        position="2"
        value="mysurname"/>
  </pages>
  <pages name="Contacts">
    <elements
        xsi:type="webapp1:InputBox"
        name="email"
        position="1"
        value="type your email"
        mask="text"/>
    <elements
        xsi:type="webapp1:InputBox"
        name="message"
        position="2"
        value="type your message"
        mask="text"/>
    <elements
        xsi:type="webapp1:Button"
        name="send"
        position="3"
        value="send"/>
  </pages>
</webapp1:App>
